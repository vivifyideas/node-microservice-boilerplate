{
  "name": "node-microservice-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "watch": "nodemon",
    "start": "npm run prod",
    "server": "babel-node ./src/bin/server",
    "server:prod": "node ./dist/bin/server",
    "dev": "cross-env NODE_ENV=development npm-run-all server",
    "prod": "cross-env NODE_ENV=production npm-run-all c lean build server:prod",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "test": "jest --runInBand --verbose",
    "coverage": "jest --coverage --runInBand --verbose",
    "eslint": "eslint src/**/*.js --ignore-pattern \"node_modules/\""
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "dotenv-expand": "^5.1.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "log4js": "^5.1.0",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-express": "^4.0.7"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.3",
    "prettier": "^1.18.2"
  }
}
